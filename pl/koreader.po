msgstr[0] "Interwał szybkości przyspieszania: 1 sekunda"
msgstr[1] "Interwał szybkości przyspieszania: %1 sekundy"
msgstr[2] "Interwał szybkości przyspieszania: %1 sekund"
msgstr[3] "Interwał szybkości przyspieszania: %1 sekund"

#: frontend/apps/reader/modules/readerhighlight.lua:851
