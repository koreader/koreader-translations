msgstr[0] "Interval zrýchľovania: 1 sekunda"
msgstr[1] "Interval zrýchľovania: %1 sekundy"
msgstr[2] "Interval zrýchľovania: %1 sekúnd"
msgstr[3] "Interval zrýchľovania: %1 sekúnd"

#: frontend/apps/reader/modules/readerhighlight.lua:851
