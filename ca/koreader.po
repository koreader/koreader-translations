msgid ""
"%1\n"
"%2\n"
"Current value: %3 (%4)\n"
"Default value: %5 (%6)"
msgstr ""
"%1\n"
"%2\n"
"Valor actual: %3 (%4)\n"
"Valor per defecte: %5 (%6)"

#: frontend/ui/data/optionsutil.lua:110
msgid ""
"%1\n"
"%2\n"
"Current value: %3 (%4)\n"
"Default value: %5"
msgstr ""
"%1\n"
"%2\n"
"Valor actual: %3 (%4)\n"
"Valor per defecte: %5"

#: frontend/ui/data/optionsutil.lua:114
msgid ""
"%1\n"
"%2\n"
"Current value: %3\n"
"Default value: %4"
msgstr ""
"%1\n"
"%2\n"
"Valor actual: %3\n"
"Valor per defecte: %4"

#: frontend/ui/data/optionsutil.lua:124
msgid ""
"Current margins:\n"
"  left:  %1\n"
"  right: %2\n"
"Default margins: not set"
msgstr ""
"Marges actuals:\n"
"  esquerre: %1\n"
"  dret: %2\n"
"Marges per defecte: sense configurar"

#: frontend/ui/data/optionsutil.lua:133
msgid ""
"Current margins:\n"
"  left:  %1\n"
"  right: %2\n"
"Default margins:\n"
"  left:  %3\n"
"  right: %4"
msgstr ""
"Marges actuals:\n"
