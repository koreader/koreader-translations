msgid ""
"%1\n"
"%2\n"
"Current value: %3 (%4)\n"
"Default value: %5"
msgstr ""
"%1\n"
"%2\n"
"Aktuális érték: %3 (%4)\n"
"Alapértelmezett érték: %5"

