msgid "(main font)"
msgstr "(основен шрифт)"

#: frontend/apps/reader/modules/readerfont.lua:487
msgid "Use main font"
msgstr "Използване на основния шрифт"

#: frontend/apps/reader/modules/readerfont.lua:490
