#: frontend/apps/reader/modules/readertoc.lua:1387
msgid "ToC entry font size"
msgstr "Dimensiune font intrare din Cuprins"

#: frontend/apps/reader/modules/readertoc.lua:1396
msgid "Show chapter length"
msgstr "Arată lungime capitol"

#: frontend/apps/reader/modules/readertoc.lua:1407
msgid "Dot leaders"
msgstr "Marcatori inițiali cu puncte"

